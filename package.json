{
  "name": "diycollections_server",
  "version": "1.0.0",
  "description": "MYSQL server for diycollections",
  "main": "./src/server.js",
  "bin": "./src/server.js",
  "scripts": {
    "start": "node ./server.js",
    "start:dev": "REACT_APP_STAGE='development' nodemon ./src/server.js"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "src/**/*"
    ],
    "targets": [
      "node13-linux-x64"
    ],
    "outputPath": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DarryllRobinson/diycollections_server.git"
  },
  "keywords": [
    "mysql",
    "node",
    "express",
    "diycollections"
  ],
  "author": "Darryll Robinson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DarryllRobinson/diycollections_server/issues"
  },
  "homepage": "https://github.com/DarryllRobinson/diycollections_server#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.6.2",
    "pkg": "^5.3.1",
    "rootpath": "^0.1.2",
    "sequelize": "^6.6.5",
    "swagger-ui-express": "^4.1.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
